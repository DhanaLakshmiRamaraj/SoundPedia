<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="@color/chat_expired_label" />
            <gradient android:angle="-90" android:startColor="@color/chat_expired_label" android:endColor="@color/chat_expired_label"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="@color/chat_expired_label" />
            <solid android:color="@color/chat_expired_label"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="@color/chat_expired_label" />
            <gradient android:angle="-90" android:startColor="@color/chat_expired_label" android:endColor="@color/chat_expired_label" />
        </shape>
    </item>
</selector>